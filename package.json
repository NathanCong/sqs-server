{
  "name": "sqs-server",
  "version": "1.0.0",
  "description": "上清所项目(服务端)",
  "repository": "https://github.com/NathanCong/sqs-server.git",
  "author": "NathanCong <congyunan@126.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "tsx watch src/main.ts",
    "start": "tsx src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^14.0.0",
    "@types/koa-bodyparser": "^4.3.12",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.4.1",
    "pg": "^8.16.3",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/koa": "^3.0.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^24.8.0",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
